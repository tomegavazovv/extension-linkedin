{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": false,
    "jsx": "react"
  },
  "include": [
    "src",
    "src/pages/Background/linkedinClient/createHeaders.js",
    "src/pages/Background/linkedinClient/getCookiesAndCsrf.js"
  ],
  "exclude": ["build", "node_modules"]
}
