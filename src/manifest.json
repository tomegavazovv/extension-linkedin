{
  "manifest_version": 3,
  "name": "Dexra",
  "description": "Curate your LinkedIn feed: Build custom lists of people, browse, and engage with their posts â€” all at one convenient place.",
  "version": "1.1.4",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "cookies",
    "storage",
    "alarms",
    "tabs"
  ],
  "host_permissions": [
    "https://www.linkedin.com/*",
    "https://app.useaware.co/*",
    "https://dexra.co/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/*",
        "https://dexra.co/*"
      ],
      "css": [
        "content.styles.css"
      ],
      "js": [
        "contentScript.bundle.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icon128.png",
        "icon48.png",
        "icon16.png"
      ],
      "matches": []
    }
  ]
}